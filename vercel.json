{
  "version": 2,
  "buildCommand": "cd website && npm install && npm run build",
  "outputDirectory": "website/.next",
  "installCommand": "cd website && npm install",
  "builds": [
    {
      "src": "website/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/website/$1"
    }
  ]
} 